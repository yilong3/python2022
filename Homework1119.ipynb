    "import turtle\n",
    "def polygon(n):\n",
    "    t = turtle.Turtle()\n",
    "    t.speed(5) # 1:slowest, 3:slow, 5:normal, 10:fast, 0:fastest\n",
    "    for i in range(n):\n",
    "        t.forward(60)\n",
    "        t.right(360/n)"
   ]
