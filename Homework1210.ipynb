    "import turtle\n",
    "t = turtle.Turtle()\n",
    "t.speed(0) # 1:slowest, 3:slow, 5:normal, 10:fast, 0:fastest\n",
    "def drawShape(n, r):\n",
    "  for i in range(n):\n",
    "    t.circle(r)\n",
    "    t.right(360.0/n)"
